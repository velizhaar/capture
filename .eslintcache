ward Componenet\r\nconst Award = ({ title, description }) => {\r\n  return (\r\n    <AwardStyle>\r\n      <h3>{title}</h3>\r\n      <div className=\"line\"></div>\r\n      <p>{description}</p>\r\n    </AwardStyle>\r\n  );\r\n};\r\n\r\nexport default MovieDetail;\r\n","E:\\react\\chapter 3\\capture\\src\\MovieState.jsx",[],"E:\\react\\chapter 3\\capture\\src\\pages\\ContactUs.jsx",[],"E:\\react\\chapter 3\\capture\\src\\index.jsx",[],"E:\\react\\chapter 3\\capture\\src\\animation.jsx",[],"E:\\react\\chapter 3\\capture\\src\\components\\Wave.jsx",[],"E:\\react\\chapter 3\\capture\\src\\components\\Toggle.jsx",[],"E:\\react\\chapter 3\\capture\\src\\components\\useScroll.jsx",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"89","replacedBy":"93"},{"ruleId":"91","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":10,"column":10,"nodeType":"97","messageId":"98","endLine":10,"endColumn":22},{"ruleId":"95","severity":1,"message":"96","line":6,"column":10,"nodeType":"97","messageId":"98","endLine":6,"endColumn":22},{"ruleId":"89","replacedBy":"99"},{"ruleId":"91","replacedBy":"100"},{"ruleId":"95","severity":1,"message":"101","line":17,"column":10,"nodeType":"97","messageId":"98","endLine":17,"endColumn":19},{"ruleId":"95","severity":1,"message":"102","line":12,"column":18,"nodeType":"97","messageId":"98","endLine":12,"endColumn":27},{"ruleId":"103","severity":1,"message":"104","line":19,"column":6,"nodeType":"105","endLine":19,"endColumn":18,"suggestions":"106"},"no-native-reassign",["107"],"no-negated-in-lhs",["108"],["107"],["108"],"no-unused-vars","'scrollReveal' is defined but never used.","Identifier","unusedVar",["107"],["108"],"'useScroll' is defined but never used.","'setMovies' is assigned a value but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'movies'. Either include it or remove the dependency array.","ArrayExpression",["109"],"no-global-assign","no-unsafe-negation"